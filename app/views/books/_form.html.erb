<%= form_with(model: @book, data: {turbo:false})  do |f| %>
  
    <p>
      <%= f.label :Book_name %><br/>
      <%= f.text_field :name %>
    </p>
  
    <p>
      <%= f.label :author %><br/>
      <%= f.text_area :author %>
    </p>

     <p>
      <%= f.label :assign %><br/>
      <%= f.text_area :assign %>
    </p>

     <p>
      <%= f.label :sell %><br/>
      <%= f.text_area :sell %>
    </p>
  
    <p>
      <%= f.submit %>
    </p>
  
  <% end %>